<style>
  @media only screen and (max-width: 767px) {.olast{order: 1 !important;}.ofirst{order: -1 !important;}}
</style>
  
<div class="row m-0 p-0 ">
    <div class="col-12 col-sm-6 col-md-12 p-0 m-0 pb-0 pr-sm-1 pr-md-0 mb-3 px-1 px-md-0">
            <div class="generic-panel">
              <h5 class="h6 panel-header ">Sobre mim</h5>
              <div class="vRow aln-center px-5 px-sm-2 px-md-3">
              <img alt="sobre-mim imagem" 
              src=" <%- conteudo.sobre[0].img[0].xs.location %>"
              srcset=" <%- conteudo.sobre[0].srcset %>"
              sizes="152px"
              
              
              class="mt-3 mb-2 about-me-image rounded-circle">
              <h5 class="h5 text-center"> <%- conteudo.sobre[0].title%></h5>
              <p class="p text-center"><%- conteudo.sobre[0].body%></p>
              </div>
            </div>
    </div>
    <!-- media -->
    <div class="col-12 media-row  olast mb-2 mb-md-3 px-1 px-md-0"> 
      <div class="hRow aln-center jus-center">
        <a href="https://www.instagram.com/envision.ingles/" target="_blank" class="mx-2 mx-sm-4 mx-md-2 mx-lg-3 p-lg-1">
                <img src="/images/instagram.png" alt="">
        </a>
        <a href="https://www.facebook.com/Envision.ingles/" target="_blank" class="mx-2 mx-sm-4 mx-md-2 mx-lg-3 p-lg-1">
                <img src="/images/facebook.png" alt="">
        </a>
        <a href="" target="_blank" class="mx-2 mx-sm-4 mx-md-2 mx-lg-3 p-lg-1" >
                <img src="/images/youtube.png" alt="">
        </a>
        <a href="https://wa.me/5522998088902" target="_blank" class="mx-2 mx-sm-4 mx-md-2 mx-lg-3 p-lg-1">
                <img src="/images/whatsapp.png" alt="">
                
        </a>
      </div>
              
    </div>
